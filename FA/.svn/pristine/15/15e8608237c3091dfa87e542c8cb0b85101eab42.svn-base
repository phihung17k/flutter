package dto;

/**
 *
 * @author Admin
 */
public class Role {
    private int id;
    private String name;

    public Role(String name) {
        this.name = name;
    }

    public int getId() {
        return id;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }
    
    
}
