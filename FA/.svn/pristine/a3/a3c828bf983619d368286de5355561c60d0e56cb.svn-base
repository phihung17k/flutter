package dto;

/**
 *
 * @author Admin
 */
public class Room {
    private int id;
    private String name;
    private float price;
    

    public Room() {

    }
    
    public Room(String name, float price) {
        this.name = name;
        this.price = price;
    }
    
    public void setId(int id) {
        this.id = id;
    }

    public int getId() {
        return id;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public float getPrice() {
        return price;
    }

    public void setPrice(float price) {
        this.price = price;
    }
    
}
